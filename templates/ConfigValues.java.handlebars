package com.reactnativeultimateconfig;
import java.util.*;

class ConfigValues {
  public static Map<String, Object> getConstants() {
    final Map<String, Object> constants = new HashMap<>();
{{#each @root}}
{{#if this.isString}}
    constants.put("{{@key}}", "{{{this.value}}}");
{{/if}}
{{#if this.isBoolean}}
    constants.put("{{@key}}", new Boolean({{{this.value}}}));
{{/if}}
{{#if this.isNumber}}
{{#if isInteger}}
    constants.put("{{@key}}", new Integer({{{this.value}}}));
{{else}}
    constants.put("{{@key}}", new Double({{{this.value}}}));
{{/if}}
{{/if}}
{{/each}}
    return constants;
  }
}
